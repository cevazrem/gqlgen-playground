{
  "id": null,
  "uid": "content-grpc-dashboard",
  "title": "content-service gRPC",
  "tags": ["gqlgen-playground", "grpc", "content-service"],
  "timezone": "",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "style": "dark",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "gRPC RPS по методам",
      "description": "Количество обработанных gRPC запросов по методам (rate grpc_server_handled_total)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (grpc_method) (rate(grpc_server_handled_total{job=\"content-service\", grpc_service=~\"$grpc_service\"}[$__rate_interval]))",
          "legendFormat": "{{grpc_method}}",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Error rate по кодам (non-OK)",
      "description": "Частота неуспешных ответов по grpc_code",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (grpc_code) (rate(grpc_server_handled_total{job=\"content-service\", grpc_service=~\"$grpc_service\", grpc_code!=\"OK\"}[$__rate_interval]))",
          "legendFormat": "{{grpc_code}}",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "p95 latency по методам (сек)",
      "description": "histogram_quantile(0.95, grpc_server_handling_seconds_bucket)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (grpc_method, le) (rate(grpc_server_handling_seconds_bucket{job=\"content-service\", grpc_service=~\"$grpc_service\"}[$__rate_interval])))",
          "legendFormat": "{{grpc_method}}",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "p99 latency по методам (сек)",
      "description": "histogram_quantile(0.99, grpc_server_handling_seconds_bucket)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum by (grpc_method, le) (rate(grpc_server_handling_seconds_bucket{job=\"content-service\", grpc_service=~\"$grpc_service\"}[$__rate_interval])))",
          "legendFormat": "{{grpc_method}}",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "gRPC started vs handled",
      "description": "Сравнение начатых и обработанных RPC",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (grpc_method) (rate(grpc_server_started_total{job=\"content-service\", grpc_service=~\"$grpc_service\"}[$__rate_interval]))",
          "legendFormat": "started {{grpc_method}}",
          "range": true
        },
        {
          "refId": "B",
          "expr": "sum by (grpc_method) (rate(grpc_server_handled_total{job=\"content-service\", grpc_service=~\"$grpc_service\"}[$__rate_interval]))",
          "legendFormat": "handled {{grpc_method}}",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 1,
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Ошибки по grpc_code (stacked)",
      "description": "Stacked error rate по кодам gRPC",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (grpc_code) (rate(grpc_server_handled_total{job=\"content-service\", grpc_service=~\"$grpc_service\", grpc_code!=\"OK\"}[$__rate_interval]))",
          "legendFormat": "{{grpc_code}}",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s",
          "custom": {
            "stacking": { "mode": "normal", "group": "A" },
            "drawStyle": "line",
            "lineWidth": 1,
            "fillOpacity": 30
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Go goroutines",
      "description": "Количество goroutine в процессе content-service",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "go_goroutines{job=\"content-service\"}",
          "legendFormat": "goroutines",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "RSS память процесса",
      "description": "process_resident_memory_bytes",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "process_resident_memory_bytes{job=\"content-service\"}",
          "legendFormat": "RSS",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "GC паузы",
      "description": "go_gc_duration_seconds_sum (rate)",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(go_gc_duration_seconds_sum{job=\"content-service\"}[$__rate_interval])",
          "legendFormat": "GC pause sum",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "label": "Prometheus",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "text": "Prometheus",
          "value": "Prometheus",
          "selected": true
        },
        "hide": 0
      },
      {
        "name": "grpc_service",
        "label": "grpc_service",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "refresh": 1,
        "includeAll": true,
        "multi": false,
        "query": "label_values(grpc_server_handled_total{job=\"content-service\"}, grpc_service)",
        "current": {
          "text": "All",
          "value": "All",
          "selected": true
        }
      }
    ]
  },
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"]
  }
}
